{
    "workflow": {
        "interpreter": "/opt/conda/bin/python3",
        "mseed_path": "/tmp/data/data/endeavour",
        "templates_path": "/tmp/data/templates/june2017_templates.tgz",
        "detections_path": "/tmp/data/data/outputs/detections/",
        "jobs_path": "/tmp/data/data/outputs/jobs/",
        "starting_cat_path": "/tmp/data/endquakes_2017.xml",
        "picks_path": "/tmp/data/data/outputs/picking/",
        "stations": ["KEMF","NCHR","ENWF","KEMO"],
        "network": "NV",
        "year": 2017
    },
    "log": {
            "verbose": 0, 
            "appendlog": false,
            "logs_path": "/tmp/data/data/outputs/logs/"
    },
    "model": {
        "pretrained": "pnw",
        "max_gap": 10,
        "default_args": {
            "overlap": 1000, 
            "blinding": [1000, 1000]
        },
        "detection_args":{
            "P_threshold": 0.0,
            "S_threshold": 0.0
        }
    },
    "environment": {
        "NPROC": 9,
        "CUDA_VISIBLE_DEVICES": [0,1,2,3],
        "OPENBLAS_NUM_THREADS": "1"
    },
    "templates": {
        "lowcut": 8.0,
        "highcut": 35.0,
        "filt_order": 4,
        "data_pad": 20.0,
        "samp_rate": 200.0,
        "length": 0.5,
        "prepick": 0.05,
        "process_len": 86400,
        "min_snr": 0.1,
        "swin": "all"
    },
    "starting_catalog": {
        "min_lat": 47.9,
        "max_lat": 48.05,
        "min_lon": -129.15,
        "max_lon": -129.05,
        "min_time": "obspy.UTCDateTime(2017,6,1,1,30,0)",
        "max_time": "obspy.UTCDateTime(2017,7,1,1,30,0)",
        "min_magnitude": 1.2,
        "max_magnitude": "NaN"
    },
    "template_matching": {
        "threshold": 8,
        "threshold_type": "MAD",
        "trig_int": 1
    }          
}