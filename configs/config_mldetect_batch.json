{
    "workflow": {
        "interpreter": "/opt/conda/bin/python3",
        "mseed_path": "/tmp/data/data/endeavour",
        "jobs_path": "/tmp/data/data/outputs/mldetect/jobs/",
        "picks_path": "/tmp/data/data/outputs/mldetect/picks/",
        "catalog_path": "/tmp/data/data/outputs/mldetect/catalogs/"
    },
    "log": {
            "verbose": 0,
            "appendlog": false,
            "logs_path": "/tmp/data/data/outputs/mldetect/logs/"
    },
    "model": {
        "picking":{
            "hourly_detection": false,
            "pretrained": "original",
            "max_gap": 20,
            "default_args": {
                "overlap": 3000,
                "blinding": [1000, 1000]
            },
            "detection_args":{
                "P_threshold": 0.1,
                "S_threshold": 0.1
            }
        },
        "association":{
            "stations": "data/stations.csv",
            "dims": ["x(km)", "y(km)", "z(km)"],
            "use_dbscan": false,
            "use_amplitude": false,
            "x(km)": [-3800, -3600],
            "y(km)": [17200, 17400],
            "z(km)": [-20, 20],
            "center": [-129, 47.5],
            "vel": {
                "p": 7.0,
                "s": 4.0
            },
            "method": "BGMM",
            "oversample_factor": 5,
            "min_picks_per_eq": 1,
            "max_sigma11": 2.0,
            "max_sigma22": 1.0,
            "max_sigma12": 1.0
        }
    },
    "environment": {
        "NTASK": 4,
        "NPROC": 8,
        "CUDA_VISIBLE_DEVICES": [],
        "OPENBLAS_NUM_THREADS": "1"
    }
}
